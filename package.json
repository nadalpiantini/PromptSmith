{
  "name": "promptsmith-mcp",
  "version": "1.0.0",
  "description": "Intelligent prompt optimization MCP server for transforming vibecoding into structured prompts",
  "main": "dist/cli.js",
  "bin": {
    "promptsmith-mcp": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "start:http": "node dist/server/http-server.js",
    "dev:http": "tsx watch src/server/http-server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "setup": "./scripts/setup.sh",
    "test:server": "node scripts/test-server.js",
    "verify": "./scripts/verify-installation.sh",
    "docs": "typedoc src/",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "docker:build": "docker build -t promptsmith-mcp .",
    "docker:run": "docker run -p 3000:3000 promptsmith-mcp"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "prompt-engineering",
    "ai",
    "llm",
    "cursor",
    "vibecoding",
    "prompt-optimization"
  ],
  "author": "PromptSmith Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.45.4",
    "dotenv": "^16.4.5",
    "liquidjs": "^10.16.1",
    "natural": "^8.0.1",
    "openai": "^4.67.3",
    "ioredis": "^5.4.1",
    "winston": "^3.14.2",
    "zod": "^3.23.8",
    "compromise": "^14.14.0",
    "uuid": "^10.0.0",
    "crypto": "^1.0.1",
    "express": "^4.19.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/promptsmith/promptsmith-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/promptsmith/promptsmith-mcp/issues"
  },
  "homepage": "https://github.com/promptsmith/promptsmith-mcp#readme"
}