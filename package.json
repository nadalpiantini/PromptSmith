{
  "name": "promptsmith-mcp",
  "version": "1.0.0",
  "description": "Intelligent prompt optimization MCP server for transforming vibecoding into structured prompts",
  "main": "dist/cli.js",
  "bin": {
    "pimpprompt": "./pimpprompt",
    "promptsmith-mcp": "./bin/promptsmith-mcp"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && node scripts/post-build.cjs",
    "build:old": "tsc && chmod +x dist/cli.js && chmod +x bin/promptsmith-mcp",
    "post-build": "node scripts/post-build.cjs",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "start:http": "node dist/server/http-server.js",
    "dev:http": "tsx watch src/server/http-server.ts",
    "web": "npm run build && npm run start:http",
    "dev:web": "npm run dev:http",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "setup": "./scripts/setup.sh",
    "test:server": "node scripts/test-server.js",
    "verify": "./scripts/verify-installation.sh",
    "verify:mcp": "node scripts/verify-mcp-patch.cjs",
    "docs": "typedoc src/",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "docker:build": "docker build -t promptsmith-mcp .",
    "docker:run": "docker run -p 3000:3000 promptsmith-mcp",
    "production:check": "node scripts/production-readiness-check.js",
    "auto-fix": "tsx tools/auto-fix-mcp.ts",
    "auto-fix:docker": "docker-compose --profile auto-fix up --build",
    "mcp:stdio": "node scripts/patch-and-run-promptsmith.cjs",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:web": "./scripts/deploy-web.sh",
    "deploy:sujeto10": "./scripts/deploy-sujeto10.sh",
    "start:production": "NODE_ENV=production npm run start:http"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "prompt-engineering",
    "ai",
    "llm",
    "cursor",
    "vibecoding",
    "prompt-optimization"
  ],
  "author": "PromptSmith Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.45.4",
    "compromise": "^14.14.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "liquidjs": "^10.16.1",
    "natural": "^8.0.1",
    "openai": "^4.67.3",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "playwright": "^1.55.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/promptsmith/promptsmith-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/promptsmith/promptsmith-mcp/issues"
  },
  "homepage": "https://github.com/promptsmith/promptsmith-mcp#readme"
}
